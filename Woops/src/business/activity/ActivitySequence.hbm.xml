<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping>
	<class name="business.activity.ActivitySequence" table="ActivitySequence">
		<!-- identifiant d'une occurrence dans la table -->
		<id
			column="id"
			name="id"
			type="integer"
		>
			<!--identifiant auto-incrementÃ© --> 
			<generator class="identity" />
		</id>
		
		<!--  -->
		<many-to-one
			class="business.activity.Activity"
			name="predecessor"
			not-null="true"
			>
			<column name="predecessor" />
		</many-to-one>
		
		<!--  -->
		<many-to-one
			class="business.activity.Activity"
			name="successor"
			not-null="true"
			>
			<column name="successor" />
		</many-to-one>
		
		

	</class>	
</hibernate-mapping>
