<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping>
	<class name="business.chocolat.Chocolat" table="chocolat">
		<id
			column="ID"
			name="id"
			type="integer"
		>
			<generator class="assigned" />
		</id>
		
		<property
			column="NOM"
			length="45"
			name="nom"
			not-null="false"
			type="string"
		 />	
		
		<property
			column="FABRICANT"
			length="45"
			name="fabricant"
			not-null="false"
			type="string"
		 />	
		 
		 <property
			column="CALORIE"
			length="5"
			name="calorie"
			not-null="false"
			type="integer"
		 />	
	</class>	
</hibernate-mapping>
