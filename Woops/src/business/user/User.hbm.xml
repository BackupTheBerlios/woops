<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping>
	<class name="business.user.User" table="User">
		<id
			column="id"
			name="id"
			type="integer"
		>
		<generator class="assigned" />
		</id>
	</class>	
</hibernate-mapping>
